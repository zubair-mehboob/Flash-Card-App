{
  "expo": {
    "name": "Flash Card",
    "slug": "final-project",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "ios": {
      "bundleIdentifier": "com.zubair.flashcard"
    },
    "android": {
      "package": "com.zubair.flashcard"
    },
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
